yload.strokeColour)\n      ctx.beginPath();\n      ctx.moveTo(payload.prevX - 1, payload.prevY - 1);\n      ctx.strokeStyle = payload.strokectxolour;\n      ctx.lineWidth = payload.lineWidth;\n      ctx.lineTo(payload.prevX, payload.prevY);\n      ctx.stroke();\n    })\n  }, [socket])\n\n\n\n\n  return (\n    <>\n      <canvas\n        onMouseDown={(e) => handleMouseDown(e)}\n        ref={canvasRef}\n        {...props}\n      ></canvas>\n      <button onClick={clearCanvas}>Clear Canvas</button>\n    </>\n  );\n}\n\nexport default Canvas;\n\n  // const [name, setName] = useState(\"\");\n  // const [room, setRoom] = useState(\"\");\n\n  // const handleClick = (e) => {\n  //   if (name === \"\" || room === \"\") {\n  //     e.preventDefault();\n  //   }\n  // }",{"ruleId":"41","replacedBy":"42"},{"ruleId":"43","replacedBy":"44"},{"ruleId":"45","severity":1,"message":"46","line":1,"column":27,"nodeType":"47","messageId":"48","endLine":1,"endColumn":36},{"ruleId":"45","severity":1,"message":"49","line":2,"column":8,"nodeType":"47","messageId":"48","endLine":2,"endColumn":22},{"ruleId":"45","severity":1,"message":"50","line":3,"column":35,"nodeType":"47","messageId":"48","endLine":3,"endColumn":41},{"ruleId":"45","severity":1,"message":"51","line":3,"column":50,"nodeType":"47","messageId":"48","endLine":3,"endColumn":54},{"ruleId":"45","severity":1,"message":"52","line":10,"column":7,"nodeType":"47","messageId":"48","endLine":10,"endColumn":15},{"ruleId":"45","severity":1,"message":"53","line":15,"column":10,"nodeType":"47","messageId":"48","endLine":15,"endColumn":13},{"ruleId":"45","severity":1,"message":"54","line":15,"column":15,"nodeType":"47","messageId":"48","endLine":15,"endColumn":21},{"ruleId":"45","severity":1,"message":"55","line":5,"column":8,"nodeType":"47","messageId":"48","endLine":5,"endColumn":14},{"ruleId":"45","severity":1,"message":"56","line":18,"column":10,"nodeType":"47","messageId":"48","endLine":18,"endColumn":14},{"ruleId":"45","severity":1,"mes